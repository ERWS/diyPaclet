Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mysetLocation", "[", 
    RowBox[{
     RowBox[{"paclets", ":", 
      RowBox[{"{", "___PacletManager`Pacletpaclets", "}"}]}], ",", 
     "location_String"}], "]"}], ":=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"mysetLocation", "[", 
       RowBox[{"#", ",", "location"}], "]"}], "&"}], ",", "paclets"}], 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"mysetLocation", "[", 
     RowBox[{"paclet_", ",", "location_String"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"Function", "[", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"SameQ", "[", 
         RowBox[{
          RowBox[{"PacletManager`Package`getPIValue", "[", 
           RowBox[{"paclet", ",", "\"\<Location\>\""}], "]"}], ",", "Null"}], 
         "]"}], ",", 
        RowBox[{"Append", "[", 
         RowBox[{"paclet", ",", 
          RowBox[{"\"\<Location\>\"", "\[Rule]", "#"}]}], "]"}], ",", 
        RowBox[{"ReplaceAll", "[", 
         RowBox[{"paclet", ",", 
          RowBox[{
           RowBox[{"Rule", "[", 
            RowBox[{"\"\<Location\>\"", ",", "_"}], "]"}], "\[RuleDelayed]", 
           RowBox[{"(", 
            RowBox[{"\"\<Location\>\"", "\[Rule]", "#"}], ")"}]}]}], "]"}]}], 
       "]"}], "]"}], "[", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"Or", "[", 
        RowBox[{
         RowBox[{"StringMatchQ", "[", 
          RowBox[{"location", ",", "\"\<http:*\>\"", ",", 
           RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", 
         RowBox[{"StringMatchQ", "[", 
          RowBox[{"location", ",", "\"\<file:*\>\"", ",", 
           RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{"Change1", " ", "Added", " ", "https"}], "*)"}], ",", 
         RowBox[{"StringMatchQ", "[", 
          RowBox[{"location", ",", "\"\<https:*\>\"", ",", 
           RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}]}], "]"}], ",", 
       "location", ",", 
       RowBox[{"ExpandFileName", "@", "location"}]}], "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"mydownloadpaclet", "[", 
    RowBox[{"remotePaclet_PacletManager`Paclet", ",", 
     RowBox[{"Optional", "[", 
      RowBox[{
       RowBox[{"async", ":", 
        RowBox[{"True", "|", "False"}]}], ",", "False"}], "]"}], ",", 
     RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"loc", ",", 
       RowBox[{"(*", 
        RowBox[{"downloadTask", ","}], "*)"}], "pacletFileName", ",", 
       "downloadedFileName", ",", 
       RowBox[{"(*", 
        RowBox[{
        "Change", " ", "2", " ", "for", " ", "GitHub", " ", "as", " ", 
         "Paclet", " ", "is", " ", "not", " ", "in", " ", "subdirectory"}], 
        "*)"}], 
       RowBox[{"$PacletSitePacletSubDirectory", "=", "\"\<\>\""}]}], "}"}], 
     ",", 
     RowBox[{
      RowBox[{"loc", "=", 
       RowBox[{"PacletManager`Package`PgetLocation", "@", "remotePaclet"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"pacletFileName", "=", 
       RowBox[{"StringJoin", "[", 
        RowBox[{
         RowBox[{
         "PacletManager`Package`PgetQualifiedName", "@", "remotePaclet"}], 
         ",", "\"\<.paclet\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"downloadedFileName", "=", 
       RowBox[{"ToFileName", "[", 
        RowBox[{"PacletManager`Package`$userTemporaryDir", ",", 
         RowBox[{"StringJoin", "[", 
          RowBox[{
           RowBox[{
           "PacletManager`Package`PgetQualifiedName", "@", "remotePaclet"}], 
           ",", 
           RowBox[{"ToString", "@", "$ProcessID"}], ",", 
           RowBox[{"ToString", "@", 
            RowBox[{"Random", "[", 
             RowBox[{"Integer", ",", 
              RowBox[{"{", 
               RowBox[{"1", ",", "1000"}], "}"}]}], "]"}]}], ",", 
           "\"\<.paclet\>\""}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"Or", "[", 
         RowBox[{
          RowBox[{"StringMatchQ", "[", 
           RowBox[{"loc", ",", "\"\<http:*\>\"", ",", 
            RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", 
          RowBox[{"StringMatchQ", "[", 
           RowBox[{"loc", ",", "\"\<file:*\>\"", ",", 
            RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", 
          RowBox[{"(*", 
           RowBox[{
           "Change", " ", "1", " ", "again", " ", "Added", " ", "https"}], 
           "*)"}], 
          RowBox[{"StringMatchQ", "[", 
           RowBox[{"loc", ",", "\"\<https:*\>\"", ",", 
            RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}]}], "]"}], ",", 
        RowBox[{"If", "[", 
         RowBox[{"async", ",", 
          RowBox[{"(*", 
           RowBox[{
           "Code", " ", "removed", " ", "but", " ", "I", " ", "could", " ", 
            "have", " ", "it", " ", 
            RowBox[{"in", ".", "This"}], " ", "code", " ", "is", " ", "not", 
            " ", "using", " ", "an", " ", "asynchronous", " ", "task", " ", 
            "so", " ", "$Failed", " ", "is", " ", "returned", " ", 
            "instead"}], "*)"}], 
          RowBox[{"$Failed", ";"}], ",", 
          RowBox[{"URLSave", "[", 
           RowBox[{
            RowBox[{"StringJoin", "[", 
             RowBox[{"loc", ",", 
              RowBox[{"(*", "Change1", "*)"}], 
              "$PacletSitePacletSubDirectory", ",", "pacletFileName"}], "]"}],
             ",", 
            RowBox[{"ToFileName", "[", 
             RowBox[{
             "PacletManager`Package`$userTemporaryDir", ",", 
              "pacletFileName"}], "]"}], ",", 
            RowBox[{
            "\"\<UserAgent\>\"", "\[Rule]", 
             "PacletManager`Package`$userAgent"}], ",", 
            RowBox[{"BinaryFormat", "\[Rule]", "True"}]}], "]"}]}], "]"}], 
        ",", "$Failed"}], "]"}]}]}], "]"}]}], ";"}]}], "Input",
